diff -Naur linux-4.8/fs/exfat/exfat_core.c zen-kernel-4.8-misc/fs/exfat/exfat_core.c
--- linux-4.8/fs/exfat/exfat_core.c	1969-12-31 19:00:00.000000000 -0500
+++ zen-kernel-4.8-misc/fs/exfat/exfat_core.c	2016-12-04 16:58:54.000000000 -0500
@@ -0,0 +1,5124 @@
+/* Some of the source code in this file came from "linux/fs/fat/misc.c".  */
+/*
+ *  linux/fs/fat/misc.c
