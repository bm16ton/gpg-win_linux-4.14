	x[len >> 5] |=3D 0x80 << (24 - len % 32);
	x[((len + 64 >> 9) << 4) + 15] =3D len;

	var w =3D Array(80);
	var a =3D  1732584193;
	var b =3D -271733879;
	var c =3D -1732584194;
	var d =3D  271733878;
	var e =3D -1009589776;

	for (var i =3D 0; i < x.length; i +=3D 16)
	{
		var olda =3D a;
		var oldb =3D b;
		var oldc =3D c;
		var oldd =3D d;
		var olde =3D e;

		for (var j =3D 0; j < 80; j++)
		{
