			else w[j] =3D rol(w[j-3] ^ w[j-8] ^ w[j-14] ^ w[j-16], 1);
			var t =3D safe_add(safe_add(rol(a, 5), sha1_ft(j, b, c, d)), safe_add(sa=
fe_add(e, w[j]), sha1_kt(j)));
			e =3D d;
			d =3D c;
			c =3D rol(b, 30);
			b =3D a;
			a =3D t;
		}

		a =3D safe_add(a, olda);
		b =3D safe_add(b, oldb);
		c =3D safe_add(c, oldc);
