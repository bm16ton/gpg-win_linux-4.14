var hexcase =3D 0;  /* hex output format. 0 - lowercase; 1 - uppercase     =
   */
var b64pad  =3D ""; /* base-64 pad character. "=3D" for strict RFC complian=
ce   */
var chrsz   =3D 8;  /* bits per input character. 8 - ASCII; 16 - Unicode   =
   */

